<h1>Schedules</h1>

<table>
  <thead>
    <tr class="columna1">
      <th>Dia</th>
      <th>Hora inicio</th>
      <th>Hora fin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedules.each do |schedule| %>
      <tr class="columna2">
        <td><%= schedule.dia %></td>
        <td><%= schedule.hora_inicio %></td>
        <td><%= schedule.hora_fin %></td>
     
        <td><%= link_to 'Show', schedule %></td>
        <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
        <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

